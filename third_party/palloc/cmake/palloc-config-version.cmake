set(palloc_version_major 2)
set(palloc_version_minor 2)
set(palloc_version_patch 7)
set(palloc_version ${palloc_version_major}.${palloc_version_minor})

set(PACKAGE_VERSION ${palloc_version})
if(PACKAGE_FIND_VERSION_MAJOR)
    if("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL "${palloc_version_major}")
        if ("${PACKAGE_FIND_VERSION_MINOR}" EQUAL "${palloc_version_minor}")
            set(PACKAGE_VERSION_EXACT TRUE)
        elseif("${PACKAGE_FIND_VERSION_MINOR}" LESS "${palloc_version_minor}")
            set(PACKAGE_VERSION_COMPATIBLE TRUE)
        else()
            set(PACKAGE_VERSION_UNSUITABLE TRUE)
        endif()
    else()
        set(PACKAGE_VERSION_UNSUITABLE TRUE)
    endif()
endif()
